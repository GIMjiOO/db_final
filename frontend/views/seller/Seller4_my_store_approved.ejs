<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">

    <meta name="description" content="">
    <meta name="author" content="Tooplate">

    <title>Moso Interior - HTML CSS Template</title>

    <!-- CSS FILES -->
    <link rel="preconnect" href="https://fonts.googleapis.com">

    <!--<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>-->

    <link href="https://fonts.googleapis.com/css2?family=League+Spartan:wght@100;300;400;600;700&display=swap" rel="stylesheet">

    <link href="css/bootstrap.min.css" rel="stylesheet">

    <link href="css/bootstrap-icons.css" rel="stylesheet">

    <link href="css/owl.carousel.min.css" rel="stylesheet">

    <link href="css/tooplate-moso-interior.css" rel="stylesheet">

    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">


    <!-- Bootstrap core CSS -->
    <!--<link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">-->


    <!-- Additional CSS Files -->
    <link rel="stylesheet" href="assets/css/fontawesome.css">
    <link rel="stylesheet" href="assets/css/templatemo-scholar.css">
    <link rel="stylesheet" href="assets/css/owl.css">
    <link rel="stylesheet" href="assets/css/animate.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css" />


</head>



<body>

    <!-- ***** Header Area Start ***** -->
    <header class="header-area header-sticky">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav">
                        <!-- ***** Logo Start ***** -->
                        <a href="#top" class="logo">
                            <h1>JUNO</h1>
                        </a>
                        <!-- ***** Logo End ***** -->

                        <!-- ***** Search Start ***** -->
                        <div class="search-input">
                            <form id="search" action="/2-Search" method="GET">
                                <input type="text" placeholder="Type Something" id="searchText" name="keyword" />
                                <i class="fa fa-search"></i>
                            </form>
                        </div>

                        <!-- ***** Search End ***** -->

                        <!-- ***** Menu Start ***** -->
                        <ul class="nav">
                            <li class="scroll-to-section"><a href="#top" class="active">Home</a></li>
                            <li><a href="/My_wishlist">Favorite</a></li>
                            <li><a href="/Cart">Cart</a></li>
                            <li><a href="/My_account_Profile">Account</a></li>
                        </ul>

                        <a class="menu-trigger">
                            <span>Menu</span>
                        </a>
                        <!-- ***** Menu End ***** -->
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- ***** Header Area End ***** -->

    <main>
        <div class="contact-us section" id="contact">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 align-self-center">
                        <div class="section-heading">
                            <h2>My Account</h2>
                            <h5><a href="/My_account_Profile">My Profile</a></h5><br>
                            <h5><a href="/My_wishlist">My Wishlist</a></h5><br>
                            <h5><a href="/My_order">My Order</a></h5><br>
                            <h5><a href="/My_Address">My Addresses</a></h5><br>
                            <!-- No link -->
                            <h5><a href="#">My Store</a></h5><br>
                            <h5><a href="/logout">Log out</a></h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <section class="section courses">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <div class="section-heading">
                            <h6>
                                <%= store.name %>
                            </h6>
                            <h2>Rating</h2>
                            <div class="d-flex justify-content-center align-items-center mt-2">
                                <% 
                    const rating = store.rating;
                    const fullStars = Math.floor(rating);
                    const halfStar = rating % 1 >= 0.25 && rating % 1 < 0.75;
                    const emptyStars = 5 - fullStars - (halfStar ? 1 : 0);
                  %>
                                    <% for(let i=0; i<fullStars; i++) { %>
                                        <i class="bi-star-fill text-warning me-1"></i>
                                        <% } %>
                                            <% if(halfStar) { %>
                                                <i class="bi-star-half text-warning me-1"></i>
                                                <% } %>
                                                    <% for(let i=0; i<emptyStars; i++) { %>
                                                        <i class="bi-star text-warning me-1"></i>
                                                        <% } %>
                                                            <span class="text-dark ms-2"><%= rating.toFixed(1) %></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <!-- Store Stats -->
        <div class="services section">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-6">
                        <div class="service-item">
                            <div class="main-content">
                                <h4>Total Product</h4>
                                <p>
                                    <%= store.totalProduct %>
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="service-item">
                            <div class="main-content">
                                <h4>Total Revenue</h4>
                                <p>
                                    <%= store.totalRevenue %>
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="service-item">
                            <div class="main-content">
                                <h4>Total Order</h4>
                                <p>
                                    <%= store.totalOrder %>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Product List -->
        <section class="section courses">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <div class="section-heading">
                            <h6>Product</h6>
                            <div class="main-button">
                                <a href="/Add_Product">Add Product</a>
                            </div>
                            <% if (products.length === 0) { %>
                                <p>You don't have any products yet. Click the “Add Product” button to add your first product and start selling!</p>
                                <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>

    <% if (products.length > 0) { %>
        <div class="section events" id="events">
            <div class="container">
                <div class="row">
                    <% products.forEach(product => { %>
                        <div class="col-lg-12 col-md-6 mb-3">
                            <div class="item">
                                <div class="row">
                                    <div class="col-lg-3">
                                        <div class="image">
                                            <img src="<%= product.imagePath %>" alt="">
                                        </div>
                                    </div>
                                    <div class="col-lg-9">
                                        <ul>
                                            <li>
                                                <span class="category"><%= product.category %></span>
                                                <h4>
                                                    <%= product.name %>
                                                </h4>
                                            </li>
                                            <li>
                                                <span>Price:</span>
                                                <h6>
                                                    <%= product.price %>
                                                </h6>
                                            </li>
                                            <li>
                                                <span>Status:</span>
                                                <h6>
                                                    <%= product.status %>
                                                </h6>
                                            </li>
                                            <li>
                                                <span>Rating:</span>
                                                <h6>
                                                    <%= product.rating.toFixed(1) %>
                                                </h6>
                                            </li>
                                            <li>
                                                <span>Flash sale:</span>
                                                <h6>
                                                    <%= product.flashSaleDate ? product.flashSaleDate : 'N/A' %>
                                                </h6>
                                            </li>
                                        </ul>
                                        <a href="/Edit_Product/<%= product.id %>"><i class="fa fa-angle-right"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <% }) %>
                </div>
            </div>
        </div>
        <% } %>


            <footer>
                <div class="container">
                    <div class="col-lg-12">
                        <p>Contact Us: <a href="mailto:Juno.Shop@gmail.com">Juno.Shop@gmail.com</a></p>
                    </div>
                </div>
            </footer>

            <!-- JAVASCRIPT FILES -->
            <script src="js/jquery.min.js"></script>
            <script src="js/bootstrap.min.js"></script>
            <script src="js/click-scroll.js"></script>
            <script src="js/jquery.backstretch.min.js"></script>
            <script src="js/owl.carousel.min.js"></script>
            <script src="js/custom.js"></script>

            <!-- Scripts -->
            <!-- Bootstrap core JavaScript -->
            <script src="vendor/jquery/jquery.min.js"></script>
            <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
            <script src="assets/js/isotope.min.js"></script>
            <script src="assets/js/owl-carousel.js"></script>
            <script src="assets/js/counter.js"></script>
            <script src="assets/js/custom.js"></script>


</body>

</html>